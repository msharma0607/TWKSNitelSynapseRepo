				"source": [
					"\n",
					"anomaly_df.write\\\n",
					".option(Constants.SERVER, \"twksnitelsynapse.sql.azuresynapse.net\")\\\n",
					".option(Constants.TEMP_FOLDER, \"abfss://twks-nitel-datalake@twksnitelstorageacc.dfs.core.windows.net/synapsesql\")\\\n",
					" .mode(\"append\")\\\n",
